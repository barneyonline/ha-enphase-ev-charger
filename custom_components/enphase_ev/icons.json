{
  "entity": {
    "binary_sensor": {
      "enphase_ev": {
        "cloud_reachable": {
          "state": {
            "on": "mdi:cloud-check",
            "off": "mdi:cloud-alert"
          }
        }
      }
    },
    "sensor": {
      "enphase_ev": {
        "connector_status": {
          "state": {
            "AVAILABLE": "mdi:ev-station",
            "CHARGING": "mdi:lightning-bolt",
            "FINISHING": "mdi:check-circle",
            "SUSPENDED": "mdi:pause-circle"
          }
        },
        "power": { "default": "mdi:flash" },
        "session_energy": { "default": "mdi:gauge" },
        "charging_level": { "default": "mdi:current-ac" },
        "session_duration": { "default": "mdi:timer-outline" },
        "last_successful_update": { "default": "mdi:clock-check-outline" },
        "cloud_latency": { "default": "mdi:timer-sand" }
      }
    },
    "number": {
      "enphase_ev": {
        "charging_amps": { "default": "mdi:current-ac" }
      }
    },
    "button": {
      "enphase_ev": {
        "start_charging": { "default": "mdi:play" },
        "stop_charging": { "default": "mdi:stop" }
      }
    }
  }
}

